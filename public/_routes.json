{
  "version": 1,
  "include": ["/*"],
  "exclude": ["/api/*"],
  "routes": [
    {
      "src": "/api/.*",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*\\.(js|css|png|jpg|jpeg|gif|ico|svg|woff|woff2|ttf|eot)$)",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}