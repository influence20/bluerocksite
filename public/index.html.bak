                </div>
                
                <!-- Savings Calculator -->
                <div id="savings-calculator" class="calculator-content active">
                    <form id="savings-calculator-form">
                        <div class="calculator-form">
                            <div class="form-group">
                                <label for="initial-amount">Initial Deposit ($)</label>
                                <input type="number" id="initial-amount" min="0" step="100" value="1000">
                            </div>
                            
                            <div class="form-group">
                                <label for="monthly-deposit">Monthly Deposit ($)</label>
                                <input type="number" id="monthly-deposit" min="0" step="50" value="100">
                            </div>
                            
                            <div class="form-group">
                                <label for="interest-rate">Annual Interest Rate (%)</label>
                                <input type="number" id="interest-rate" min="0" step="0.1" value="3.5">
                            </div>
                            
                            <div class="form-group">
                                <label for="years">Time Period (Years)</label>
                                <input type="number" id="years" min="1" max="50" step="1" value="10">
                            </div>
                        </div>
                        
                        <button type="submit" class="btn">Calculate</button>
                    </form>
                    
                    <div id="savings-calculator-results" class="calculator-results" style="display: none;">
                        <h3>Results</h3>
                        
                        <div class="result-item">
                            <span class="result-label">Future Value:</span>
                            <span id="future-value" class="result-value">$0.00</span>
                        </div>
                        
                        <div class="result-item">
                            <span class="result-label">Total Deposits:</span>
                            <span id="total-deposits" class="result-value">$0.00</span>
                        </div>
                        
                        <div class="result-item">
                            <span class="result-label">Interest Earned:</span>
                            <span id="interest-earned" class="result-value">$0.00</span>
                        </div>
                        
                        <div class="calculator-chart">
                            <canvas id="savings-chart"></canvas>
                        </div>
                    </div>
                </div>
                
                <!-- Investment Calculator -->
                <div id="investment-calculator" class="calculator-content">
                    <form id="investment-calculator-form">
                        <div class="calculator-form">
                            <div class="form-group">
                                <label for="initial-investment">Initial Investment ($)</label>
                                <input type="number" id="initial-investment" min="300" step="100" value="300">
                            </div>
                            
                            <div class="form-group">
                                <label for="annual-contribution">Annual Contribution ($)</label>
                                <input type="number" id="annual-contribution" min="0" step="500" value="2000">
                            </div>
                            
                            <div class="form-group">
                                <label for="return-rate">Expected Annual Return (%)</label>
                                <input type="number" id="return-rate" min="0" step="0.5" value="7">
                            </div>
                            
                            <div class="form-group">
                                <label for="investment-years">Investment Period (Years)</label>
                                <input type="number" id="investment-years" min="1" max="50" step="1" value="20">
                            </div>
                            
                            <div class="form-group">
                                <label for="risk-level">Risk Level</label>
                                <select id="risk-level">
                                    <option value="low">Conservative</option>
                                    <option value="medium" selected>Moderate</option>
                                    <option value="high">Aggressive</option>
                                </select>
                            </div>
                        </div>
                        
                        <button type="submit" class="btn">Calculate</button>
                    </form>
                    
                    <div id="investment-calculator-results" class="calculator-results" style="display: none;">
                        <h3>Results</h3>
                        
                        <div class="result-item">
